{
  "name": "feliz",
  "version": "0.0.50",
  "description": "The quickest way to kickstart the development of your app.",
  "main": "lib/feliz.js",
  "author": "Héctor Menéndez <etor@gik.mx>",
  "license": "MIT",
  "analyze": true,
  "keywords": [
    "rx",
    "observable",
    "framework",
    "minimal"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gikmx/feliz"
  },
  "bugs": {
    "url": "https://github.com/gikmx/feliz/issues"
  },
  "engines": {
    "node": ">=6.2.0"
  },
  "scripts": {
    "start": "npm-watch",
    "docs": "jsdoc2md -t ./template.hbs 'lib/**/*.js' > README.md",
    "test": "tape ./test/feliz.js | tap-spec",
    "cover": "npm run cover:stats && npm run cover:send",
    "cover:check": "npm run cover:stats && istanbul check-coverage",
    "cover:stats": "istanbul cover tape -- ./test/feliz.js",
    "cover:send": "cat ./coverage/lcov.info | coveralls",
    "precommit": "npm test && npm run cover:check && npm run docs && git add --force README.md && feliz.hooks pre-commit"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "feliz.error": "0.0.3",
    "feliz.util": "0.0.14",
    "rxjs": "^5.0.0-beta.10"
  },
  "devDependencies": {
    "coveralls": "^2.11.12",
    "feliz.hooks": "0.0.5",
    "feliz.test": "0.0.10",
    "husky": "^0.11.5",
    "istanbul": "^0.4.4",
    "jsdoc-to-markdown": "^1.3.6",
    "npm-watch": "git://github.com/hectormenendez/npm-watch.git",
    "tap-spec": "^4.1.1"
  },
  "watch": {
    "test": {
      "patterns": [
        "lib",
        "test"
      ],
      "extensions": "js",
      "inherit": true
    },
    "docs": {
      "patterns": [
        "lib",
        "template.hbs"
      ],
      "extensions": "js",
      "inherit": true
    }
  }
}
